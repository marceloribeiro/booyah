{{model_name}} = {{model_name.capitalize()}}.find(self.params['id'])
        return self.respond_to(
          html=lambda: self.render({ "{{model_name.underscore()}}": {{model_name.underscore()}} }),
          json=lambda: self.render({ "{{model_name}}": {{model_name.camelize()}}Serializer({{model_name}}).to_dict() })
        )